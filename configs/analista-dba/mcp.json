      "command": "uvx",
      "args": ["awslabs.postgres-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "aurora-mysql": {
      "command": "uvx",
      "args": ["awslabs.mysql-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "aurora-dsql": {
      "command": "uvx",
      "args": ["awslabs.aurora-dsql-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      }
    },
    "documentdb": {
      "command": "uvx",
      "args": ["awslabs.documentdb-mcp-server@latest"],
